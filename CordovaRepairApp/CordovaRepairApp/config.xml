<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.CordovaRepairApp" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Contoso Property Management Repair App</name>
  <description>A Cordoba app that targets iPad Air, Nexus 9, and Microsoft Surface that allows Contoso Property Management repair people to conduct repairs and report their work.</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Canviz</author>
  <content src="incidents.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.1" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="Orientation" value="landscape" />
  <preference name="android-minSdkVersion" value="19" />
  <preference name="android-maxSdkVersion" value="19" />
  <preference name="android-targetSdkVersion" value="19" />
  <preference name="target-device" value="tablet" />
  <preference name="BackupWebStorage" value="none" />
  <preference name="deployment-target" value="8.1" />
  <vs:plugin name="org.apache.cordova.inappbrowser" />
  <vs:plugin name="de.appplant.cordova.plugin.email-composer" version="0.8.2dev" />
  <vs:plugin name="org.apache.cordova.camera" version="0.3.5-dev" />
  <vs:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
  <vs:platformSpecificValues>
    <vs:platformSpecificWidget platformName="windows" version="1.0.0.0">
      <vs:name>Contoso Property Management Repair App</vs:name>
    </vs:platformSpecificWidget>
  </vs:platformSpecificValues>
</widget>